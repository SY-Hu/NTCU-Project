	<script>
		function userchoose() {
			var tracepeople = document.getElementById('datepicker').value;
			var time = tracepeople.split("/");
			var mm = time[0];
			var dd = time[1];
			var yyyy = time[2];
			window.location.href = "/sensor?user=" + yyyy + '-' + mm + '-'+ dd;
		}
		var temp = max; 
		console.log(temp);
	</script>

