		<!-- Content -->
		<div class="site-content">
			<div class="content-area py-1">
				<div class="container-fluid">
					<h4>Sensor Data</h4>

					<ol class="breadcrumb no-bg mb-1">
						<li class="breadcrumb-item"><a href="/home">Home</a></li>
						<li class="breadcrumb-item active">Sensor Data</li>
					</ol>

					<div class="box box-block bg-white">
						<h5>Sensor Data</h5>
						<p class="font-90 text-muted mb-1">Search car sensor data record</p>
						<br>
						<div class="mb-1">
								<div class="input-group">
									   <input type="text" class="form-control datepicker-multiple" placeholder="mm/dd/yyyy" id="datepicker">
									<span class="input-group-addon"><i class="fa fa-calendar-o"></i></span>
								</div>
								<button type="button" onclick="userchoose()" class="btn btn-danger w-min-sm mb-0-25 waves-effect waves-light">Search</button>
						</div>
						
						<table class="table">
							<tr>
								<th>Date</th>
								<th>Latitude</th>
								<th>Longitude</th>
								<th>Temperature</th>
								<th>Humidity</th>
							</tr>
							<% for ( var i = 0 ; i < sen.length ; i++){ %>
								<tr>
									<td>
										<%= sen[i].date.toLocaleDateString() +" "+ sen[i].date.toLocaleTimeString()  %>
									</td>
									<td>
										<%= sen[i].gpslat  %>
									</td>
									<td>
										<%= sen[i].gpslng  %>
									</td>

									<td>
										<%= sen[i].temperature  %>
									</td>
									<td>
										<%= sen[i].humidity  %>
									</td>

								</tr>
								<% } %>
						</table>
						<h4>Max: <%= max %></h4>
						<h4>Min: <%= min %></h4>
						<h4>Avg: <%= avg %></h4>
					</div>


				</div>
			</div>
		</div>
