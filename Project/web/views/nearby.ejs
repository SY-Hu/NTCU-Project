	<script>
		var lat2, lon2;

		function Positioning() {

			if (navigator.geolocation) {
				browserSupportFlag = true;
				navigator.geolocation.getCurrentPosition(function (position) {
					lat2 = position.coords.latitude;
					lon2 = position.coords.longitude;
				}, function () {
					handleNoGeolocation(browserSupportFlag);
				});
			} else {
				browserSupportFlag = false;
				handleNoGeolocation(browserSupportFlag);
			}
		}

		function nearby() {
			var distan = document.getElementById('distance').value;
			var entry, lat, lng, tracename, time, register;
			if (navigator.geolocation) {
				browserSupportFlag = true;
				navigator.geolocation.getCurrentPosition(function (position) {
					lat2 = position.coords.latitude;
					lon2 = position.coords.longitude;
				}, function () {
					handleNoGeolocation(browserSupportFlag);
				});
			} else {
				browserSupportFlag = false;
				handleNoGeolocation(browserSupportFlag);
			}
			var at = new google.maps.LatLng(lat2, lon2);
			var map = new google.maps.Map(document.getElementById('map'), {
				zoom: 16,
				center: at
			});
			var marker = new google.maps.Marker({
				position: at,
				map: map,
				label: 'You'
			});
			var person = '<%- JSON.stringify(data) %>';
			var jsonObj = JSON.parse(person);
			var people = '<%- JSON.stringify(name) %>';
			var jsonObjpeo = JSON.parse(people);
			console.log(jsonObj[1].date);
			for (var j = 0; j < jsonObjpeo.length; j++) {
				for (var i = jsonObj.length - 1; i >= 0; i--) {
					lat = jsonObj[i].gpslat;
					lng = jsonObj[i].gpslng;
					tracename = jsonObj[i].username;
					console.log('hi');
					console.log(lat);
					console.log(lng);
					console.log(tracename);
					if (tracename != '<%= suser.username %>' && tracename == jsonObjpeo[j].username) {
						var lat1 = lat;
						var lon1 = lng;
						var R = 6371;
						var dLat = (lat2 - lat1) * Math.PI / 180;
						var dLon = (lon2 - lon1) * Math.PI / 180;
						var a = Math.sin(dLat / 2) * Math.sin(dLat / 2) + Math.cos(lat1 * Math.PI / 180) * Math.cos(lat2 * Math.PI / 180) *
							Math.sin(dLon / 2) * Math.sin(dLon / 2);
						var c = 2 * Math.atan2(Math.sqrt(a), Math.sqrt(1 - a));
						var d = R * c;
						if (d < distan) {
							var uluru = new google.maps.LatLng(lat, lng);
							var oldmarker = new google.maps.Marker({
								position: uluru,
								label: tracename,
								map: map
							});



						}
						break;
					}
				}
			}



			if (navigator.geolocation) {

				browserSupportFlag = true;
				navigator.geolocation.getCurrentPosition(function (position) {
					var initialLocation = new google.maps.LatLng(position.coords.latitude, position.coords.longitude);
					var marker = new google.maps.Marker({
						position: initialLocation,
						map: map,
						label: 'You'
					});
				}, function () {
					handleNoGeolocation(browserSupportFlag);
				});
			} else {
				browserSupportFlag = false;
				handleNoGeolocation(browserSupportFlag);
			}


			function handleNoGeolocation(errorFlag) {
				if (errorFlag == true) {
					alert("地圖定位失敗");
				} else {
					alert("您的瀏覽器不支援定位服務");
				}
				initialLocation = taipei;
				map.setCenter(initialLocation);
			}
		}
		window.onload = Positioning();
	</script>

