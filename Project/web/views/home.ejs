	<script>
		var taipei = new google.maps.LatLng(25.09108, 121.5598);
		var initialLocation;
		var browserSupportFlag = new Boolean();
		var counter = 0;
		var now_lat;
		var now_lng;
		function initMap() {
			var element1 = document.getElementById("lat");
			var element2 = document.getElementById("lng");
			var uluru = new google.maps.LatLng(element1.value, element2.value);
			var map = new google.maps.Map(document.getElementById('map'), {
				zoom: 16,
				center: uluru
			});
			var marker = new google.maps.Marker({
				position: uluru,
				map: map
			});

		}
		var userid = document.getElementById("username");
		console.log(userid);

		function newlocation() {
			/*var element1 = document.getElementById("people");
			people_val = element1.options[element1.selectedIndex].value;*/
			setTimeout("newlocation();", 15000);
			if (navigator.geolocation) {
				browserSupportFlag = true;
				navigator.geolocation.getCurrentPosition(function (position) {

					initialLocation = new google.maps.LatLng(position.coords.latitude, position.coords.longitude);
					//map.setCenter(initialLocation);

					var map = new google.maps.Map(document.getElementById('map'), {
						zoom: 16,
						center: initialLocation
					});
					var marker = new google.maps.Marker({
						position: initialLocation,
						map: map
					});

					var Today = new Date();
					/*$.getScript(
						"https://docs.google.com/forms/d/e/1FAIpQLScfXrpQGrwE18P1DxAed_YrYc2Th-Ty-sFs7kLxzx4Glu59Sw/formResponse?entry.431392795=" +
						initialLocation + "&entry.1231185025=" + Today.getFullYear() + (Today.getMonth() + 1) + Today.getDate() +
						"&entry.382930322=" + people_val + "&submit=Submit");*/
					;
					$.ajax({
						type: 'POST',
						url: "/home",
						data: {
							gpslat: position.coords.latitude,
							gpslng: position.coords.longitude,
							username: userid.value
						},
						dataType: 'json',
						success: function (data) {
							console.log(data);
							console.log('process sucess');
						},

						error: function () {
							console.log('process error');
						},
					});
				}, function () {
					handleNoGeolocation(browserSupportFlag);
				});
			} else {
				browserSupportFlag = false;
				handleNoGeolocation(browserSupportFlag);
			}

			function handleNoGeolocation(errorFlag) {
				if (errorFlag == true) {
					alert("地圖定位失敗");
				} else {
					alert("您的瀏覽器不支援定位服務");
				}
				initialLocation = taipei;
				map.setCenter(initialLocation);
			}


		}
		window.onload = newlocation();
	</script>

